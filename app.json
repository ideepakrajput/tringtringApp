{
  "expo": {
    "name": "Tring Tring",
    "slug": "tring-tring",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ],
      [
        "@react-native-google-signin/google-signin"
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ideepakrajput.tringtring"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "myGoogleApp",
    "extra": {
      "eas": {
        "projectId": "73451e45-27e1-4572-8648-12fe212f8793"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-6067634275916377~7802227182"
  }
}